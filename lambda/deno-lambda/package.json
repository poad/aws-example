{
  "name": "deno-lambda",
  "version": "0.1.0",
  "bin": "bin/deno-lambda.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format-check": "biome format bin lib lambda",
    "format": "biome format --write bin lib lambda",
    "check": "biome check bin lib lambda",
    "check-fix": "biome check --apply-suggested bin lib lambda",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.7",
    "aws-cdk": "^2.118.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
