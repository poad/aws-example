{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/lambda-examples.js",
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint './{lambda,bin,lib}/**/*.ts'",
    "lint:fix": "eslint --fix './{lambda,bin,lib}/**/*.ts'"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.142",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.2",
    "aws-cdk": "^2.150.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.140.0-alpha.0",
    "aws-cdk-lib": "^2.150.0",
    "aws-lambda": "^1.0.7",
    "cdk-ecr-deployment": "^3.0.78",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
