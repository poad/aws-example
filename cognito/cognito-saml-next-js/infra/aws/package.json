{
  "name": "cognito-saml-next-js",
  "version": "0.1.0",
  "bin": "bin/cognito-saml-next-js.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-suggested bin lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "prebuild": "pnpm install",
    "all": "pnpm lint && pnpm build"
  },
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@swc/helpers": "^0.5.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.5.1",
    "aws-cdk": "^2.92.0",
    "regenerator-runtime": "^0.14.0",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.395.0",
    "aws-cdk-lib": "^2.92.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  }
}
