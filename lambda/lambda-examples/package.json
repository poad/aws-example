{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/lambda-examples.js",
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts bin lambda lib",
    "lint:fix": "eslint --fix --ext .ts bin lambda lib"
  },
  "devDependencies": {
    "@swc/core": "*",
    "@swc/helpers": "*",
    "@types/aws-lambda": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/prettier": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "aws-cdk": "*",
    "esbuild": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-prettier": "*",
    "jest": "*",
    "prettier": "*",
    "regenerator-runtime": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.50.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.50.0-alpha.0",
    "@aws-cdk/aws-lambda-python-alpha": "^2.50.0-alpha.0",
    "@aws-example-common/cognito-singin": "../../common/cognito-singin/",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "cdk-ecr-deployment": "*",
    "constructs": "*",
    "cross-fetch": "*",
    "source-map-support": "*"
  },
  "packageManager": "yarn@3.3.0",
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
