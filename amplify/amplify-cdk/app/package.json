{
  "name": "amplify-cdk-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "eslint-check": "eslint -c ../../../eslint.config.js './src/**/*{.ts,.js,.tsx,.jsx}'",
    "eslint-fix": "eslint -c ../../../eslint.config.js --fix './src/**/*{.ts,.js,.tsx,.jsx}'",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint": "pnpm prettier-check && pnpm eslint-check",
    "lint-fix": "pnpm prettier-format && pnpm eslint-fix",
    "clean": "rimraf build",
    "analyze": "ANALYZE=true next build",
    "all": "pnpm install && yarn lint && yarn build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "autoprefixer": "^10.4.21",
    "next": "^15.5.4",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.14",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/core": "^0.16.0",
    "@eslint/js": "^9.37.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.18.9",
    "@types/react": "^19.2.2",
    "eslint": "^9.37.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "styled-jsx": "^5.1.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
