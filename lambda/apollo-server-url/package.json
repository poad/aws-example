    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.33",
    "aws-cdk": "^2.1019.1",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.202.0",
    "constructs": "^10.4.2",
    "graphql": "^16.11.0",
