{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/lambda-examples.js",
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts bin lambda lib",
    "lint:fix": "eslint --fix --ext .ts bin lambda lib"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "aws-cdk": "^2.113.0",
    "esbuild": "^0.18.20",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.100.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.100.0-alpha.0",
    "@aws-cdk/aws-lambda-python-alpha": "2.100.0-alpha.0",
    "aws-cdk-lib": "^2.113.0",
    "aws-lambda": "^1.0.7",
    "cdk-ecr-deployment": "^2.5.38",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
