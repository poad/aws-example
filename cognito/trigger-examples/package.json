    "aws-lambda": "^1.0.7",
    "constructs": "^10.2.61",
    "cross-fetch": "^3.1.6",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
