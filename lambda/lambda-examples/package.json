{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/lambda-examples.js",
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts bin lambda lib",
    "lint:fix": "eslint --fix --ext .ts bin lambda lib"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "aws-cdk": "^2.133.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.129.0-alpha.0",
    "aws-cdk-lib": "^2.133.0",
    "aws-lambda": "^1.0.7",
    "cdk-ecr-deployment": "^3.0.35",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
