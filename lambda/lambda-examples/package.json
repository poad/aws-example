{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "lambda-examples": "bin/lambda-examples.js"
  },
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts bin && eslint --ext .ts lambda && eslint --ext .ts lib",
    "lint:fix": "eslint --fix --ext .ts bin && eslint --fix --ext .ts lambda && eslint --fix --ext .ts lib"
  },
  "devDependencies": {
    "@types/aws-lambda": "*",
    "@types/jest": "*",
    "@types/jsonwebtoken": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "aws-cdk": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "jest": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "latest",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "latest",
    "@aws-cdk/aws-lambda-python-alpha": "latest",
    "@aws-sdk/client-cognito-identity": "*",
    "@aws-sdk/client-cognito-identity-provider": "*",
    "@aws-sdk/client-sts": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "cdk-ecr-deployment": "*",
    "constructs": "*",
    "cross-fetch": "*",
    "jsonwebtoken": "*",
    "source-map-support": "*"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
