{
  "name": "lambda-examples",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/lambda-examples.js",
  "scripts": {
    "build": "tsc bin/*.ts lib/*.ts",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts bin lambda lib",
    "lint:fix": "eslint --fix --ext .ts bin lambda lib"
  },
  "devDependencies": {
    "@swc/core": "^1.3.70",
    "@swc/helpers": "^0.5.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "aws-cdk": "^2.88.0",
    "esbuild": "^0.18.15",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "regenerator-runtime": "^0.13.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.87.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.87.0-alpha.0",
    "@aws-cdk/aws-lambda-python-alpha": "2.87.0-alpha.0",
    "aws-cdk-lib": "^2.88.0",
    "aws-lambda": "^1.0.7",
    "cdk-ecr-deployment": "^2.5.30",
    "constructs": "^10.2.69",
    "cross-fetch": "^3.1.8",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
