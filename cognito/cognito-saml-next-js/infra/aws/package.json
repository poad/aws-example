{
  "name": "cognito-saml-next-js",
  "version": "0.1.0",
  "bin": "bin/cognito-saml-next-js.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-suggested bin lib",
    "lint": "yarn format-check && yarn check",
    "lint-fix": "yarn format && yarn check-fix",
    "clean": "rimraf build",
    "prelint": "yarn install",
    "prebuild": "yarn install",
    "all": "yarn lint && yarn build"
  },
  "devDependencies": {
    "@swc/core": "*",
    "@swc/helpers": "*",
    "@types/aws-lambda": "*",
    "@types/node": "*",
    "aws-cdk": "*",
    "regenerator-runtime": "*",
    "rome": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "constructs": "*",
    "source-map-support": "*"
  },
  "resolutions": {
    "json5": ">=2.2.3"
  },
  "packageManager": "yarn@3.3.1"
}
