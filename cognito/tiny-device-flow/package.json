{
  "name": "tiny-device-flow",
  "version": "0.1.0",
  "bin": {
    "tiny-device-flow": "bin/tiny-device-flow.js"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx lambda"
  },
  "devDependencies": {
    "@types/aws-lambda": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/randombytes": "*",
    "@types/uuid": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "aws-cdk": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "jest": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.25.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.25.0-alpha.0",
    "@aws-sdk/client-cognito-identity-provider": "*",
    "@aws-sdk/client-dynamodb": "*",
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/util-dynamodb": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "constructs": "*",
    "cross-fetch": "*",
    "randombytes": "*",
    "source-map-support": "*",
    "uuid": "*"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
