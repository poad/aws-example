{
  "name": "infra",
  "private": true,
  "version": "0.0.1",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@middy/core": "*",
    "@types/aws-lambda": "*",
    "@types/jest": "*",
    "@types/jsonwebtoken": "*",
    "@types/log4js": "*",
    "@types/node": "*",
    "aws-cdk": "*",
    "jest": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.35.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.35.0-alpha.0",
    "@aws-lambda-powertools/logger": "*",
    "@aws-lambda-powertools/metrics": "*",
    "@aws-lambda-powertools/tracer": "*",
    "@aws-sdk/client-cognito-identity": "*",
    "@aws-sdk/client-cognito-identity-provider": "*",
    "@aws-sdk/client-sts": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "constructs": "*",
    "cross-fetch": "*",
    "jsonwebtoken": "*",
    "log4js": "*",
    "source-map-support": "*"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
