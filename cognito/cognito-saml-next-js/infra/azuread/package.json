{
  "name": "azuread",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-suggested bin lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "prebuild": "pnpm install",
    "all": "pnpm lint && pnpm build",
    "upgrade": "pnpm add \"cdktf@*\" \"cdktf-cli@*\"",
    "upgrade:next": "pnpm add cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@cdktf/provider-azuread": "^7.0.4",
    "cdktf": "^0.16.3",
    "constructs": "^10.2.69"
  },
  "devDependencies": {
    "@swc/core": "^1.3.72",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.4.5",
    "regenerator-runtime": "^0.13.11",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
