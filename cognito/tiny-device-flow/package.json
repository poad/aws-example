    "@aws-sdk/client-dynamodb": "*",
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/util-dynamodb": "*",
    "aws-cdk-lib": "*",
    "aws-lambda": "*",
    "constructs": "*",
    "cross-fetch": "*",
    "randombytes": "*",
    "source-map-support": "*",
    "uuid": "*"
  },
  "packageManager": "yarn@3.3.1",
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
